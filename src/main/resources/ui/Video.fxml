      <Button fx:id="playVideo" layoutX="30.0" layoutY="430.0" mnemonicParsing="false" onMouseClicked="#playVideo" text="播放" />
      <Button fx:id="pauseVideo" layoutX="97.0" layoutY="430.0" mnemonicParsing="false" onMouseClicked="#pauseVideo" text="暂停" />
      <Button fx:id="next" layoutX="154.0" layoutY="430.0" mnemonicParsing="false" onAction="#playNext" text="下一个" />
      <Text fx:id="name" layoutX="70.0" layoutY="54.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" wrappingWidth="401.5">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
   </children>
</AnchorPane>
